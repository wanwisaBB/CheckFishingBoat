                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ชื่อเรือ/แพปลา"
                    android:id="@+id/textView77"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp" />

                <EditText
                    android:layout_width="@dimen/edittext1"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText59"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:inputType="text"
                    android:paddingTop="0dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ที่อยู่"
                    android:id="@+id/textView78"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp" />

                <EditText
                    android:layout_width="@dimen/edittext1"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText60"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:inputType="text"
                    android:paddingTop="0dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="จังหวัด"
                    android:id="@+id/textView79"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp" />

                <EditText
                    android:layout_width="@dimen/edittext1"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText61"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:inputType="text"
                    android:paddingTop="0dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="วันที่แจ้งเรือเข้า"
                    android:id="@+id/textView80" />

                <EditText
                    android:layout_width="@dimen/edittext2"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText62"
                    android:inputType="text"
                    android:paddingTop="0dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="เวลาที่แจ้งเข้า"
                    android:id="@+id/textView81" />

                <EditText
                    android:layout_width="@dimen/edittext2"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText63"
                    android:inputType="text"
                    android:paddingTop="0dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="จำนวน"
                    android:id="@+id/textView83" />

                <EditText
                    android:layout_width="@dimen/edittext2"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText64"
                    android:inputType="text"
                    android:paddingTop="0dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ชนิดสัตว์น้ำที่จับได้"
                    android:id="@+id/textView82" />

                <EditText
                    android:layout_width="@dimen/edittext2"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText65"
                    android:inputType="text"
                    android:paddingTop="0dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="รายละเอียด"
                    android:id="@+id/textView84" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText66" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="รายชื่อผู่ควบคุมเรือ"
                    android:id="@+id/textView85" />

                <EditText
                    android:layout_width="@dimen/edittext2"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText67"
                    android:inputType="text"
                    android:paddingTop="0dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="หมายเลยประจำตัวผู้คุมเรือ"
                    android:id="@+id/textView86" />

                <EditText
                    android:layout_width="@dimen/edittext2"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText68"
                    android:inputType="text"
                    android:paddingTop="0dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="ย้อนกลับ"
                    android:id="@+id/button16" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
